{
    "title": "td-vocab-description:security",
    "description": "Provides additional (human-readable) information based on a default language.",
    "$schema ": "http://json-schema.org/draft-06/schema#",
    "is-complex": true,
    "type": "object",
    "properties": {
        "securityDefinitions": {
            "type": "object",
            "minProperties": 1,
            "additionalProperties": {
                "$ref": "#/definitions/securityScheme"
            }
        }
    },
    "required": [
        "securityDefinitions"
    ],
    "additionalProperties": true,
    "definitions": {
        "securityScheme": {
            "type": "object",
            "if": {
                "properties": {
                    "description": {
                        "type": "string"
                    }
                },
                "required": ["description"]
            },
            "then": {
                "const": "td-vocab-description:security=pass"
            }
        },
        "url": {
            "type": "string",
            "format": "uri",
            "pattern": "(([^:/?#]+):)?(//([^/?#]*))?([^?#]*)(([^#]*))?(#(.*))?"
        }
    }
}