{
    "id": "urn:digest",
    "@context": "https://www.w3.org/2019/wot/td/v1",
    "name": "MyLampThing",
    "description": "Valid TD demonstrating how to use the digest scheme",
    "securityDefinitions": {
        "digest_sc": {
            "scheme": "digest",
            "qop": "auth",
            "in": "header",
            "name": "myParameter"
        }
    },
    "security": ["digest_sc"],
    "properties": {
        "status": {
            "type": "string",
            "forms": [{
                "href": "https://mylamp.example.com/status"
            }]
        }
    }
}