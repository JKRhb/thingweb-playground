{
    "id": "urn:dev:wot:com:example:servient:lamp",
    "name": "MyLampThing",
    "description": "Warning TD since there is no security key for form 0 of overheating event but the others have it only in interaction",
    "properties": {
        "status": {
            "type": "string",
            "forms": [{
                "href": "https://mylamp.example.com/status",
                "security": [{
                    "scheme": "nosec"
                }]
            }]
        }
    },
    "actions": {
        "toggle": {
            "forms": [{
                "href": "https://mylamp.example.com/toggle",
                "security": [{
                    "scheme": "nosec"
                }]
            }]
        }
    },
    "events": {
        "overheating": {
            "data": {
                "type": "string"
            },
            "forms": [{
                "href": "https://mylamp.example.com/oh",
                "subProtocol": "longpoll",
                                "security": [{
                                    "scheme": "nosec"
                                }]
            },
        {
            "href": "https://mylamp.example.com/oh",
            "subProtocol": "longpoll"
        }]
        }
    }

}