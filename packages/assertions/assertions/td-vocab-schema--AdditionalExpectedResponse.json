{
    "title": "td-vocab-schema--AdditionalExpectedResponse",
    "description": "success: Signals if an additional response should not be considered an error.",
    "$schema ": "http://json-schema.org/draft/2019-09/schema#",
    "is-complex": true,
    "type": "object",
    "properties": {
        "forms": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "#/$defs/form_element"
            }
        },
        "properties": {
            "type": "object",
            "additionalProperties": {
                "$ref": "#/$defs/property_element"
            }

        },
        "actions": {
            "type": "object",
            "additionalProperties": {
                "$ref": "#/$defs/action_element"
            }
        },
        "events": {
            "type": "object",
            "additionalProperties": {
                "$ref": "#/$defs/event_element"
            }
        }
    },
    "$defs": {
        "property_element": {
            "type": "object",
            "properties": {
                "forms": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "#/$defs/form_element"
                    }
                }
            },
            "required": [
                "forms"
            ],
            "additionalProperties": true
        },
        "action_element": {
            "type": "object",
            "properties": {
                "forms": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "#/$defs/form_element"
                    }
                }
            },
            "required": [
                "forms"
            ],
            "additionalProperties": true
        },
        "event_element": {
            "type": "object",
            "properties": {
                "forms": {
                    "type": "array",
                    "minItems": 1,
                    "items": {
                        "$ref": "#/$defs/form_element"
                    }
                }
            },
            "required": [
                "forms"
            ],
            "additionalProperties": true
        },
        "form_element": {
            "type": "object",
            "if": {
                "required": ["additionalResponses"],
                "properties": {
                     "additionalResponses": {
                        "oneOff": [
                            {
                                "$ref": "#/$defs/additionalResponse_element"
                            },
                            {
                                "type": "array",
                                "items": {
                                    "$ref": "#/$defs/additionalResponse_element"
                                }
                            }
                        ]
                     }
                }
                                   
            },
            "then": {
                "const": "td-vocab-schema--AdditionalExpectedResponse=pass"
            }
        },
        "additionalResponse_element":{
            "type": "object",
            "properties": {
                "contentType": {
                    "type": "string"
                },
                "schema": {
                    "type": "string"
                },
                "success":{
                    "type":"boolean"
                }
            },
            "required":["schema"]
        }
    }
}